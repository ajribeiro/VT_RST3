<?xml version="1.0" encoding="ISO-8859-1"?>
<config>

  <tree>
     <delim>/</delim>
     <path>rfc</path>
  </tree>

<xmldoc>
  <root type="env">URLBASE</root>
  <tagdb>
    <tag>
      <name>rfc</name>
      <start>&lt;a &attr;&gt;</start>
      <end>&lt;/a&gt;</end>
    </tag>
</tagdb>


  <xml>
    <element>
    <name>id</name>  
    <default type="text">None</default>
    </element>
  </xml>
  <xml>
    <element>
    <name>title</name>
    <default type="text">None</default>
    </element>
  </xml>
  <xml>
    <element>
    <name>author</name>
    <default type="text">None</default>
    </element>
  </xml>

 <xml>
    <element>
    <name>summary</name>
    <default type="text">None</default>
    </element>
  </xml>
  <xml>
    <element>
    <name>body</name>
    <default type="text">None</default>
    </element>
  </xml>  
 <xml>
    <element>
    <name>reference</name>
    <default type="text">None</default>
    <action>&xml;&lt;br&gt;</action>
    </element>
  </xml>


  <xml>
    <element>
    <name>revision</name>
    <default type="text">None</default>
    <action>&xml;&lt;br&gt;</action>
    </element>
  </xml>



</xmldoc>

<map>
     <entry>
        <search>ID</search>
        <replace>id</replace>
     </entry>
    <entry>
        <search>TITLE</search>
        <replace>title</replace>
     </entry>
    <entry>
        <search>AUTHOR</search>
        <replace>author</replace>
     </entry>

     <entry>
        <search>SUMMARY</search>
        <replace>summary</replace>
     </entry>
     <entry>
        <search>BODY</search>
        <replace>body</replace>
     </entry>
     <entry>
        <search>REFERENCE</search>
        <replace>reference</replace>
     </entry>
     <entry>
        <search>REVISION</search>
        <replace>revision</replace>
     </entry>
</map>
  
<external type="file">rfcname.xml</external>
<script type="file">rfc.sk</script>

 <match>
   <element>id</element>
 </match>

</config>








