<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
  <tree>
     <delim>/</delim>
     <path>tutorial/content/page</path>
  </tree>


  <xmldoc>
    <tagdb type="file">tutorial.tagdb.xml</tagdb>
    <root type="env">URLBASE</root>
    <xml>

      <element>
      <name>project</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>title</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>location</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>author</name>  
      <default type="text">None</default>
      </element>
      <element>
      <name>summary</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>id</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>revision</name>
      <default type="text">None</default>
      <action>&xml;&lt;br&gt;</action>
      </element>

      <element>
      <name>body</name>  
      <default type="text">None</default>
      </element>



    </xml>
  </xmldoc>

  <map>

     <entry>
        <search>PROJECT</search>
        <replace>project</replace>
     </entry>

     <entry>
        <search>TITLE</search>
        <replace>title</replace>
     </entry>

     <entry>
        <search>LOCATION</search>
        <replace>location</replace>
     </entry>

     <entry>
        <search>AUTHOR</search>
        <replace>author</replace>
     </entry>
     <entry>
        <search>SUMMARY</search>
        <replace>summary</replace>
     </entry>

     <entry>
        <search>REVISION</search>
        <replace>revision</replace>
     </entry>


     <entry>
        <search>ID</search>
        <replace>id</replace>
     </entry>

     <entry>
        <search>BODY</search>
        <replace>body</replace>
     </entry>

 

  </map>

  <external type="file">tutorialname.xml</external>
  <external type="file">tutorialnav.xml</external>
 
  
  <script type="file">tutorial.sk</script>

  <match>
     <element>id</element>
  </match>

</config>


