<?xml version="1.0" encoding="ISO-8859-1"?>
<config>

  <tree>
     <delim>/</delim>
     <path>index/entry</path>
  </tree>

<xmldoc>
  <root type="env">URLBASE</root>
  <xml>
    <element>
    <name>name</name>  
    <default type="text">None</default>
    </element>
    <element>
    <name>title</name>  
    <default type="text">None</default>
    </element>
    <element>
    <name>location</name>  
    <default type="text">index.html</default>
    </element>

  </xml>
</xmldoc>

<map>
     <entry>
        <search>NAME</search>
        <replace>name</replace>
     </entry>

     <entry>
        <search>TITLE</search>
        <replace>title</replace>
     </entry>

     <entry>
        <search>LOCATION</search>
        <replace>location</replace>
     </entry>
</map>

    
  <script type="file">tmp/doc.index.main.sk</script>
  <match>
     <element>name</element>
  </match>

</config>








