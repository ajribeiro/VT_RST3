<?xml version="1.0" encoding="ISO-8859-1"?>
<config>

  <tree>
     <delim>/</delim>
     <path>index/section</path>
  </tree>

<xmldoc>
  <root type="env">URLBASE</root>
  <xml>
    <element>
    <name>name</name>  
    <default type="text">None</default>
    </element>
    <element>
    <name>url</name>  
    <default type="text">None</default>
    </element>
  </xml>
</xmldoc>

<map>
     <entry>
        <search>NAME</search>
        <replace>name</replace>
     </entry>

     <entry>
     <search>URL</search>
        <replace>url</replace>
     </entry>


</map>

    
 <script type="file">tmp/doc.section.main.sk</script>
 <external type="file">/tmp/name.xml</external>


  <match>
     <element>name</element>
  </match>

</config>








