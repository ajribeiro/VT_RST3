#ifndef _fitacflm_H
#define _fitacflm_H

#ifndef PULSE_SIZE
#define PULSE_SIZE 256
#endif

struct datapoints
{
  double *x;
  double *y;
  double *ey;
  double mag;
};
struct exdatapoints
{
  int lagnum;
  double phase;
  double lagpwr;
};

void lmfit(struct RadarParm *prm,struct RawData *ptr,
                struct FitData *fit,struct FitBlock *fblk);
double getguessex(int goodcnt, int nlags, int nslopes, struct exdatapoints * exdata,
                  int tfreq, int mpinc);
int singlefit(int m, int n, double *p, double *deviates,
                        double **derivs, void *private)

#endif
